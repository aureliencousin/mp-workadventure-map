{
  "name": "workadventure-map-starter-kit",
  "version": "3.1.5",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "update:map": "cp -R ~/workspace/guild/work-adventure-map/map.json .",
    "update:tilesets": "cp -R ~/workspace/guild/work-adventure-map/tilesets/* ./tilesets",
    "update:src": "cp -R ~/workspace/guild/work-adventure-map/src/* ./src",
    "update": "npm run update:map && npm run update:tilesets && npm run update:src"
  },
  "devDependencies": {
    "@types/node": "^17.0.36",
    "typescript": "^4.5.4",
    "vite": "^2.9.13",
    "wa-map-optimizer-vite": "^1.1.5"
  },
  "dependencies": {
    "@workadventure/iframe-api-typings": "^1.12.10",
    "@workadventure/scripting-api-extra": "^1.4.4"
  }
}
